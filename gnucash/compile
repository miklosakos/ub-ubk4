#!/bin/sh -eux

ub_compile \
    -DCMAKE_INSTALL_LIBDIR=/usr/lib \
    -DCMAKE_INSTALL_SYSCONFDIR=/usr/etc \
    -DWITH_AQBANKING=OFF \
	-DWITH_PYTHON=ON \
    -DHAVE_GWEN_GTK3=ON \
    -DCOMPILE_GSCHEMAS=OFF \
    -DGTEST_ROOT=gtest/googletest \
    -DGMOCK_ROOT=gtest/googlemock

cd gnucash-docs
ub_compile
